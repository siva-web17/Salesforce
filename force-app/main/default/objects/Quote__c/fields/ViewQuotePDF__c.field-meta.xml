<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ViewQuotePDF__c</fullName>
    <description>Formula field used to display the PDF Url in a more user friendly way.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(QuotePDF__c)),HYPERLINK( QuotePDF__c , IMAGE("/resource/QuotePDF", "PDF")),'')</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>View Quote PDF</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
